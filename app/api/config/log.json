{
  "appenders": [
    {
      "type": "console"
    },
    {
      "category": "app",
      "type": "dateFile",
      "filename": "./logs/tmp-tests.log",
      "pattern": "-yyyy-MM-dd",
      "alwaysIncludePattern": false,
      "layout": {
        "type": "pattern",
        "pattern": "[%d] [%p] - %m%n"
      }
    }
  ]
}